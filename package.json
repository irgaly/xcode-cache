{
  "name": "xcode-cache",
  "version": "1.8.1",
  "main": "dist/main/index.js",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "ncc build src/main.ts -o dist/main --source-map --license licenses.txt && ncc build src/post.ts -o dist/post --source-map --license licenses.txt && ln -sf ../lib dist/",
    "main": "ts-node src/main.ts",
    "post": "ts-node src/post.ts"
  },
  "dependencies": {
    "@actions/cache": "4.0.0",
    "@actions/core": "1.11.1",
    "@actions/exec": "1.1.1",
    "@actions/glob": "0.5.0",
    "@actions/github": "6.0.0"
  },
  "devDependencies": {
    "@types/node": "20.17.16",
    "@vercel/ncc": "0.38.0",
    "ts-node": "10.9.1",
    "typescript": "5.6.2"
  }
}
